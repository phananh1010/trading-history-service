ALPACA_API_PAPER_KEY=YOUR_ALPACA_PAPER_KEY
ALPACA_API_PAPER_SECRET=YOUR_ALPACA_PAPER_SECRET
ALPACA_ENV=paper
ALPACA_DATA_FEED=iex

ALPACA_API_LIVE_ENDPOINT=https://api.alpaca.markets
ALPACA_API_LIVE_KEY=YOUR_ALPACA_LIVE_KEY
ALPACA_API_LIVE_SECRET=YOUR_ALPACA_LIVE_SECRET

SCHWAB_API_APPKEY=YOUR_SCHWAB_APPKEY
SCHWAB_API_SECRET=YOUR_SCHWAB_SECRET

TAVILY_API_KEY=YOUR_TAVILY_API_KEY
APILAYER_API_KEY=YOUR_APILAYER_API_KEY
